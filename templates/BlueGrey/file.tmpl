[websvn-test:error]
   <div id="error">[websvn:error]</div>
[websvn-else]
   [websvn:curdirlinks] - [lang:REV] <b>[websvn:rev]</b>
   <p>
    [websvn-test:goyoungestlink]
     [websvn:goyoungestlink] -
    [websvn-endtest]
    [websvn-test:mimelink]
     [websvn:mimelink] -
    [websvn-endtest]
    [websvn-test:difflink]
     [websvn:difflink] -
    [websvn-endtest]
     [websvn:blamelink] -
    [websvn-test:downloadlink]
     [websvn:downloadlink] -
    [websvn-endtest]
     [websvn:loglink]
   </p>
   <hr />
  [websvn-test:warning]
  <div class="warning">[websvn:warning]</div>
  [websvn-else]
   [websvn-getlisting]
   <hr />
  [websvn-endtest]
[websvn-endtest]
